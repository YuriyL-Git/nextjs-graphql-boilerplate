    "dotenv": "^16.0.1",
    "eslint-plugin-type-graphql": "^1.0.0",
    "fs": "^0.0.1-security",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-request": "^4.3.0",
    "micro": "^9.3.4",
    "next": "12.2.2",
    "next-auth": "^4.10.0",
    "node-watch": "^0.7.3",
    "nodemailer": "^6.7.7",
    "praphql": "^0.1.1",
