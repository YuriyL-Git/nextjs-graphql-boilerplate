{
  "name": "next-graphql-dogs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'yarn migrate' 'yarn next dev' 'yarn codegen'",
    "build": "next build",
    "codegen": "graphql-codegen -r dotenv/config --watch",
